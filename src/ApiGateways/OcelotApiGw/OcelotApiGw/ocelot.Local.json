{
  "Routes": [
    //Catalog API
    {
      "DownstreamPathTemplate": "/api/v1/Products",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5000"
        }
      ],
      "UpstreamPathTemplate": "/Products",
      "UpstreamhttpsMethod": [ "GET", "POST", "PUT" ],
      "SwaggerKey": "Products"
    },
    {
      "DownstreamPathTemplate": "/api/v1/Products/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5000"
        }
      ],
      "UpstreamPathTemplate": "/Products/{id}",
      "UpstreamhttpsMethod": [ "GET", "DELETE" ],
      "SwaggerKey": "Products"
    },
    {
      "DownstreamPathTemplate": "/api/v1/Products/Category",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5000"
        }
      ],
      "UpstreamPathTemplate": "/Products/Category",
      "UpstreamhttpsMethod": [ "GET" ],
      "SwaggerKey": "Products"
    },
    {
      "DownstreamPathTemplate": "/api/v1/Products/SubCategory",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5000"
        }
      ],
      "UpstreamPathTemplate": "/Products/SubCategory",
      "UpstreamhttpsMethod": [ "GET" ],
      "SwaggerKey": "Products"
    },
    {
      "DownstreamPathTemplate": "/api/v1/Products/Name",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5000"
        }
      ],
      "UpstreamPathTemplate": "/Products/Name",
      "UpstreamhttpsMethod": [ "GET" ],
      "SwaggerKey": "Products"
    }

  ],
  "SwaggerEndPoints": [
    {
      "Key": "Products",
      "Config": [
        {
          "Name": "Products API",
          "Version": "v1",
          "Url": "https://localhost:5000/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5010"
  }
}
